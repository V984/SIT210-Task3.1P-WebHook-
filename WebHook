int led1 = D6; 

int led2 = D7; 


void setup() {


    pinMode(led1, OUTPUT);
    pinMode(led2, OUTPUT);

}

void loop() {
    digitalWrite(led1, HIGH);
    digitalWrite(led2, HIGH);

    delay(30000);

    digitalWrite(led1, LOW);
    digitalWrite(led2, LOW);
    
  String data = String(20);
  // Trigger the integration
  Particle.publish("Light", data, PUBLIC);

    delay(30000);


}
